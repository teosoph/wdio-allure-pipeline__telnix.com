{
  "name": "wdio-allure-pipeline__telnix.com",
  "version": "1.0.0",
  "description": "wdio project with allure reporting and pipeline to GitPages for telnix.com",
  "main": "index.js",
  "scripts": {
    "test": "npx wdio config/wdio.conf.js",
    "test:chrome:headless": "npx wdio config/wdio.chrome.headless.conf.js",
    "test:chrome": "npx wdio config/wdio.chrome.conf.js",
    "test:chrome:TC-001/002": "npx wdio config/wdio.chrome.conf.js --spec registration_tryForFree.spec.js",
    "test:chrome:TC-003/004": "npx wdio config/wdio.chrome.conf.js --spec registration_signUp.spec.js",
    "test:chrome:TC-005": "npx wdio config/wdio.chrome.conf.js --spec visibility_footerItems.spec.js",
    "test:chrome:TC-006": "npx wdio config/wdio.chrome.conf.js --spec redirection_termsPage.spec.js",
    "test:firefox:headless": "npx wdio config/wdio.firefox.headless.conf.js",
    "test:firefox": "npx wdio config/wdio.firefox.conf.js",
    "test:firefox:TC-001/002": "npx wdio config/wdio.firefox.conf.js --spec registration_tryForFree.spec.js",
    "test:firefox:TC-003/004": "npx wdio config/wdio.firefox.conf.js --spec registration_signUp.spec.js",
    "test:firefox:TC-005": "npx wdio config/wdio.firefox.conf.js --spec visibility_footerItems.spec.js",
    "test:firefox:TC-006": "npx wdio config/wdio.firefox.conf.js --spec redirection_termsPage.spec.js",
    "allure:generate": "npx allure generate allure-results --clean",
    "report": "allure generate --clean ./allure-results && allure open"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/teosoph/wdio-allure-pipeline__telnix.com.git"
  },
  "keywords": [],
  "author": "TEO",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/teosoph/wdio-allure-pipeline__telnix.com/issues"
  },
  "homepage": "https://github.com/teosoph/wdio-allure-pipeline__telnix.com#readme",
  "devDependencies": {
    "@types/chai": "^4.2.22",
    "@types/mocha": "^8.2.3",
    "@wdio/allure-reporter": "^7.24.0",
    "@wdio/cli": "^7.24.0",
    "@wdio/firefox-profile-service": "^7.24.0",
    "@wdio/local-runner": "^7.24.0",
    "@wdio/mocha-framework": "^7.24.0",
    "@wdio/selenium-standalone-service": "^7.24.0",
    "@wdio/spec-reporter": "^7.24.0",
    "chromedriver": "^105.0.0",
    "cross-env": "7.0.3",
    "wdio-chromedriver-service": "^7.3.2",
    "wdio-docker-service": "^3.2.1"
  },
  "dependencies": {
    "allure-commandline": "^2.18.1",
    "chai": "^4.3.4",
    "cross-env": "^7.0.3",
    "mocha": "^9.2.0"
  }
}
